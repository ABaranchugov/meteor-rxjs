{
 "ignore": [
    "**/node_modules/**",
    "**/dist/**",
    "**/docs/**",
    "**/examples/**"
  ],
  "test": [
      "**/test/**",
      "**/tests/**",
      "**/spec/**",
      "**/specs/**",
      "**/*.test.ts",
      "**/*.test.js",
      "**/*.spec.js",
      "**/*.spec.ts"
    ],
  "critics": {
    "lint": {"engine": "tslint"}
  },
    "dependencies": {
      "mute": [
        "rxjs",
        "jsdoc-to-markdown"
      ],
      "unused-ignores": [
        "zone.js",
        "reflect-metadata",
        "meteor-typings",
        "@types/*"
      ]
    }
}
